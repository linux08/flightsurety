{
  "name": "flightsurety",
  "version": "1.0.0",
  "description": "",
  "directories": {
    "test": "test"
  },
  "repository": "",
  "license": "MIT",
  "scripts": {
    "build": "truffle compile",
    "dapp": "webpack-dev-server --mode development --config webpack.config.dapp.js",
    "dapp:prod": "webpack --mode production  --config webpack.config.dapp.js",
    "dev": "lite-server",
    "test": "truffle test ./test/flightSurety.js",
    "test-ganache": "TRUFFLE_TEST=true npm run ganache-cli:test",
    "test:gas": "GAS_REPORTER=true npm test",
    "test:coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "ganache-cli:test": "scripts/ganache-cli.sh",
    "console": "truffle console",
    "abi:extract": "truffle-extract --output abi/ --keys abi",
    "bytecode:extract": "truffle-bytecode extract -o bytecode",
    "bytecode:extract:new": "truffle-bytecode extract -o bytecode_new",
    "bytecode:compare": "npm run bytecode:extract:new && truffle-bytecode compare bytecode bytecode_new",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:fix": "npm run lint:js:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "server": "rm -rf ./build/server && webpack --config webpack.config.server.js"
  },
  "author": "David Abimbola",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.5",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "bignumber.js": "8.0.2",
    "css-loader": "^1.0.0",
    "express": "4.16.4",
    "file-loader": "3.0.1",
    "ganache-cli": "^6.4.1",
    "hardhat": "^2.12.4",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "solhint": "3.3.7",
    "solidity-coverage": "^0.5.11",
    "start-server-webpack-plugin": "2.2.5",
    "style-loader": "^0.23.1",
    "superstatic": "6.0.3",
    "truffle": "5.0.6",
    "truffle-bytecode-manager": "^1.1.1",
    "truffle-extract": "^1.2.1",
    "truffle-flattener": "^1.3.0",
    "truffle-hdwallet-provider": "1.0.2",
    "web3": "1.0.0-beta.37",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.0",
    "dotenv": "^16.0.3",
    "install": "^0.13.0",
    "npm": "^8.19.2",
    "truffle-hdwallet-provider": "^1.0.17",
    "web3": "^1.8.0"
  },
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=18.0.0"
  }
}
